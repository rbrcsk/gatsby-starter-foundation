{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "dom",
      "esnext",
    ],
    "jsx": "preserve",
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "typeRoots": [
      "./types",
      "../../node_modules/@types"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "./types",
    "./gatsby-api",
    "./gatsby-node.ts"
  ]
}